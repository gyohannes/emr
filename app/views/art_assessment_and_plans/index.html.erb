<p id="notice"><%= notice %></p>

<h1>Art Assessment And Plans</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Art patient</th>
      <th>Arv eleigibility criteria</th>
      <th>Plan</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @art_assessment_and_plans.each do |art_assessment_and_plan| %>
      <tr>
        <td><%= art_assessment_and_plan.date %></td>
        <td><%= art_assessment_and_plan.art_patient %></td>
        <td><%= art_assessment_and_plan.ARV_eleigibility_criteria %></td>
        <td><%= art_assessment_and_plan.plan %></td>
        <td><%= link_to 'Show', art_assessment_and_plan %></td>
        <td><%= link_to 'Edit', edit_art_assessment_and_plan_path(art_assessment_and_plan) %></td>
        <td><%= link_to 'Destroy', art_assessment_and_plan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

