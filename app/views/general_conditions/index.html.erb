<p id="notice"><%= notice %></p>

<h1>General Conditions</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Art patient</th>
      <th>Vital signs anrtopometry and functional level</th>
      <th>Symptom screen</th>
      <th>Pregnancy status</th>
      <th>Physical exam</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @general_conditions.each do |general_condition| %>
      <tr>
        <td><%= general_condition.date %></td>
        <td><%= general_condition.art_patient %></td>
        <td><%= general_condition.vital_signs_anrtopometry_and_functional_level %></td>
        <td><%= general_condition.symptom_screen %></td>
        <td><%= general_condition.pregnancy_status %></td>
        <td><%= general_condition.physical_exam %></td>
        <td><%= link_to 'Show', general_condition %></td>
        <td><%= link_to 'Edit', edit_general_condition_path(general_condition) %></td>
        <td><%= link_to 'Destroy', general_condition, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New General Condition', new_general_condition_path %>
